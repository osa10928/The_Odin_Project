<% provide(:subtitle, "Create New Event!") %>

<%= form_for(@event, url: events_new_path) do |f| %>

	<%= f.label :name %>
	<%= f.text_field :name %><br>

	<%= f.label :place %>
	<%= f.text_field :place %><br>

	<%= f.label :day %>
	<%= f.date_field :day %><br>

	<%= f.label :description %>
	<%= f.text_field :desc %><br>

	<%= f.submit "Create Event!" %>

<% end %>